<?xml version="1.0" encoding="utf-8" ?>
<Patch>

    <Operation Class="PatchOperationSequence">
        <operations>

            <!--===== IOTA PSYKER =====-->
            <li Class="PatchOperationAddModExtension">
                <xpath>Defs/GeneDef[defName = "BEWH_IotaPsyker"]</xpath>
                <value>
                    <li Class="Core40k.DefModExtension_TraitAndGeneOpinion">
                        <godOpinion>
                            <li>
                                <god>Tzeentch</god>
                                <opinion>1</opinion>
                            </li>
                        </godOpinion>
                        <godWontGift>
                            <li>Khorne</li>
                        </godWontGift>
                    </li>
                    <li Class="Mutations40k.DefModExtension_ChaosMutation">
                        <givenBy>
                            <li>Tzeentch</li>
                            <li>Slaanesh</li>
                            <li>Nurgle</li>
                            <li>Undivided</li>
                        </givenBy>
                        <selectionWeight>0.1</selectionWeight>
                        <isBeneficial>True</isBeneficial>
                    </li>
                </value>
            </li>

            <!--===== EPSILON PSYKER =====-->
