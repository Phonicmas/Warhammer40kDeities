<?xml version="1.0" encoding="utf-8"?>
<Defs>

    <!-- Unfathomable Gaze -->
    <GeneDef ParentName="BEWH_BeneficialMutationBase">
        <defName>BEWH_SlaaneshUnfathomableGaze</defName>
        <label>mutation: Unfathomable gaze</label>
        <description>The pawn's eyes has mutated, giving them the ability to temporary stun anyone looking at them as they're mesmerized by the pawns eyes.</description>
        <iconPath>UI/Icons/Genes/UnfathomableGaze</iconPath>
        <displayOrderInCategory>440</displayOrderInCategory>
        <abilities>
            <li>BEWH_UnfathomableGaze</li>
        </abilities>
        <modExtensions>
            <li Class="VanillaGenesExpanded.GeneExtension">
                <backgroundPathXenogenes>UI/Icons/Genes/GeneBackground_Slaanesh</backgroundPathXenogenes>
            </li>
            <li Class="Deities40k.DefModExtension_DeityGift">
                <isBeneficial>True</isBeneficial>
                <requiredAcceptance>Acknowledged</requiredAcceptance>
                <isConsideredMutation>true</isConsideredMutation>
            </li>
        </modExtensions>
    </GeneDef>


    <!-- Alluring Presence -->
    <GeneDef ParentName="BEWH_BeneficialMutationBase">
        <defName>BEWH_SlaaneshAlluringPresence</defName>
        <label>mutation: Alluring presence</label>
        <description>This pawn has mutated aesthetically pleasing features and been instilled with great charm.</description>
        <iconPath>UI/Icons/Genes/AlluringPresence</iconPath>
        <displayOrderInCategory>440</displayOrderInCategory>
        <customEffectDescriptions>
            <li>Other pawns think highly of carriers of this gene.</li>
        </customEffectDescriptions>
        <aptitudes>
            <Social>3</Social>
        </aptitudes>
        <lovinMTBFactor>0.75</lovinMTBFactor>
        <modExtensions>
            <li Class="VanillaGenesExpanded.GeneExtension">
                <backgroundPathXenogenes>UI/Icons/Genes/GeneBackground_Slaanesh</backgroundPathXenogenes>
            </li>
            <li Class="Deities40k.DefModExtension_DeityGift">
                <isBeneficial>True</isBeneficial>
                <isConsideredMutation>true</isConsideredMutation>
            </li>
            <li Class="Deities40k.DefModExtension_SituationalThought" />
        </modExtensions>
    </GeneDef>

</Defs>