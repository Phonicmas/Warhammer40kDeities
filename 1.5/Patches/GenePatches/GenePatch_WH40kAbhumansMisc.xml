<?xml version="1.0" encoding="utf-8" ?>
<Patch>

    <Operation Class="PatchOperationFindMod">
        <mods>
            <li>Warhammer 40.000 - Abhumans</li>
        </mods>
        <match Class="PatchOperationSequence">
            <operations>

                <li Class="PatchOperationAddModExtension">
                    <xpath>Defs/GeneDef[defName = "BEWH_NavigtorWarpClose"]</xpath>
                    <value>
                        <li Class="Deities40k.DefModExtension_DeityGift">
                        </li>
                    </value>
                </li>

                <li Class="PatchOperationAddModExtension">
                    <xpath>Defs/GeneDef[defName = "BEWH_BeastmanChaosConnection"]</xpath>
                    <value>
                        <li Class="Deities40k.DefModExtension_DeityGift">
                        </li>
                    </value>
                </li>

                <li Class="PatchOperationAdd">
                    <xpath>Defs/Deities40k.DeityDef[defName = "BEWH_DeityKhorne"]/genesLike</xpath>
                    <value>
                        <li>BEWH_NavigtorWarpClose</li>
                        <li>BEWH_BeastmanChaosConnection</li>
                    </value>
                </li>
                <li Class="PatchOperationAdd">
                    <xpath>Defs/Deities40k.DeityDef[defName = "BEWH_DeityNurgle"]/genesLike</xpath>
                    <value>
                        <li>BEWH_NavigtorWarpClose</li>
                        <li>BEWH_BeastmanChaosConnection</li>
                    </value>
                </li>
                <li Class="PatchOperationAdd">
                    <xpath>Defs/Deities40k.DeityDef[defName = "BEWH_DeitySlaanesh"]/genesLike</xpath>
                    <value>
                        <li>BEWH_NavigtorWarpClose</li>
                        <li>BEWH_BeastmanChaosConnection</li>
                    </value>
                </li>
                <li Class="PatchOperationAdd">
                    <xpath>Defs/Deities40k.DeityDef[defName = "BEWH_DeityTzeentch"]/genesLike</xpath>
                    <value>
                        <li>BEWH_NavigtorWarpClose</li>
                        <li>BEWH_BeastmanChaosConnection</li>
                    </value>
                </li>
                <li Class="PatchOperationAdd">
                    <xpath>Defs/Deities40k.DeityDef[defName = "BEWH_DeityUndivided"]/genesLike</xpath>
                    <value>
                        <li>BEWH_NavigtorWarpClose</li>
                        <li>BEWH_BeastmanChaosConnection</li>
                    </value>
                </li>

            </operations>
        </match>
    </Operation>


</Patch>